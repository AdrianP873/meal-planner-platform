{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "node": true      
    },
    "rules": {
        "quotes": ["error", "double"],
        "semi": ["error", "always"],
        "no-var": 0
    }
}



